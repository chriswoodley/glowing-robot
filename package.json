{
  "name": "@cw/platform",
  "version": "1.0.0",
  "private": "true",
  "license": "MIT",
  "workspaces": [
    "packages/*",
    "apps/*"
  ],
  "scripts": {
    "custom:start": "yarn --cwd apps/custom start",
    "custom:build": "yarn --cwd apps/custom build",
    "cwoodley:start": "yarn --cwd apps/cwoodley-com/frontend start",
    "cwoodley:build": "yarn --cwd apps/cwoodley-com/frontend build",
    "cwoodley:dev": "yarn --cwd apps/cwoodley-com/frontend dev",
    "cwoodley:lint": "yarn --cwd apps/cwoodley-com/frontend lint",
    "design-system:start": "yarn --cwd packages/design-system start",
    "design-system:storybook": "yarn --cwd packages/design-system storybook",
    "dev:custom": "npm-run-all --parallel design-system:start design-system:storybook custom:start",
    "dev:cwoodley": "npm-run-all --parallel design-system:start design-system:storybook cwoodley:dev"
  },
  "devDependencies": {
    "npm-run-all": "^4.1.5"
  }
}
